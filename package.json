{
  "name": "mathjax-node-server",
  "version": "1.0.2",
  "description": "Render MathJax on server",
  "main": "index.js",
  "repository": "https://github.com/sparanoid/mathjax-node-server.git",
  "author": "Tunghsiao Liu <t@sparanoid.com>",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "release": "npx release-it"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "git": {
      "commitArgs": "-S",
      "tagArgs": "-s",
      "tagAnnotation": "chore: release ${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": true
    }
  },
  "dependencies": {
    "mathjax": "^3.2.0"
  }
}
